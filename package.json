{
  "name": "gigya",
  "version": "0.0.0-pre-alpha.0",
  "license": "MIT",
  "scripts": {
    "build": "nx run-many --target=build --all",
    "serve": "nx run-many --target=serve --all",
    "test": "nx run-many --target=test --all",
    "e2e": "nx run-many --target=e2e --all",
    "demo": "pnpm --filter demo",
    "pack-all": "pnpm -r exec pnpm pack --pack-destination tmp",
    "pnpm-pack": "pnpm pack --pack-destination tmp",
    "release": "nx release  --skip-publish version preid pre-alpha --verbose --projects web types loader --dry-run ",
    "publish": " pnpm publish --access public --tag pre-alpha  --dry-run --tag pre-alpha --no-git-checks --report-summary --filter wc",
    "version": "npm version minor --preid pre-alpha"
  },
  "private": true,
  "devDependencies": {
    "@xstate/fsm": "^2.1.0",
    "atomico": "^1.79.2",
    "@atomico/hooks": "^4.2.0",
    "@atomico/scaffold": "^2.9.5",
    "@atomico/tsconfig": "^1.1.2",
    "@atomico/vite": "^2.18.0",
    "@nx/devkit": "19.0.2",
    "@nx/eslint": "19.0.2",
    "@nx/eslint-plugin": "19.0.2",
    "@nx/js": "19.0.2",
    "@nx/node": "19.0.2",
    "@nx/playwright": "19.0.2",
    "@nx/vite": "19.0.2",
    "@nx/web": "19.0.2",
    "@nx/workspace": "19.0.2",
    "@playwright/test": "^1.36.0",
    "@types/node": "18.16.9",
    "@typescript-eslint/eslint-plugin": "^7.3.0",
    "@typescript-eslint/parser": "^7.3.0",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.3.1",
    "eslint": "~8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-playwright": "^0.15.3",
    "http-server": "^14.1.1",
    "jsdom": "~22.1.0",
    "lit": "^3.1.3",
    "nx": "19.0.2",
    "prettier": "^2.6.2",
    "tslib": "^2.6.2",
    "typescript": "~5.4.2",
    "verdaccio": "^5.0.4",
    "vite": "~5.2.11",
    "vite-plugin-dts": "~3.8.1",
    "vitest": "^1.6.0"
  },
  "nx": {
    "includedScripts": []
  },
  "packageManager": "pnpm@8.15.8",
  "pnpm": {
    "overrides": {
      "vite": "~5.2.11"
    },
    "workspaceLayout": "auto",
    "packageExtensions": {
      "atomico": {
        "peerDependencies": {
          "@atomico/vite": "*"
        }
      }
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "vite": "*"
      },
      "ignoreMissing": [
        "@algolia/client-search",
        "postcss",
        "search-insights"
      ]
    }
  }
}
